insert into cart (coustomerId,    product_id,       image,    vendormail,     product_name,   color,   category,    size,    new_price,    cost_price)\n"
                + " values(" + cart.getCoustomerId() + "," + cart.getProduct_id() +",'"+cart.getImage()+ "','" + cart.getVendormail()
				+ "','" + cart.getProduct_name() + "','" + cart.getColor() + "','" + cart.getCategory() + "','" + cart.getSize() + "'," 
				+ cart.getNew_price() + "," + cart.getCost_price() + ")";"


