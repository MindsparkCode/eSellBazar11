create table products(
   productcode int NOT NULL,
   productname VARCHAR(50) ,  
   color VARCHAR(25) ,
   size varchar(5),
   quantity int,
   unitprice int,
   cash_memo varchar(15),
   company varchar(50),
   date_time datetime default now(),
   description varchar(200),
   PRIMARY KEY (productcode)
);


