drop table vendor;
create table vendor(
  id  int NOT NULL AUTO_INCREMENT,
  name varchar(25),
  company  varchar(25),
  address varchar(25),
  district varchar(25),
  thana varchar(25) ,
  postcode varchar(10),
  nid varchar(25),
  phone varchar(25) ,
  email varchar(25) ,
  agriment varchar(25) ,
  date_time datetime default now(),
   PRIMARY KEY (id)
);

