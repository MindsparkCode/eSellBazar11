create table emp(
  id  int NOT NULL AUTO_INCREMENT,
  name varchar(25),
  nid varchar(25),
  address varchar(25),
  emp_id varchar(255) NOT NULL,
  salary double(20,2) NOT NULL,
  bonus double(20,2) DEFAULT NULL,
  commision double(20,2) DEFAULT NULL,
  absentfee double(20,2) DEFAULT NULL,
  PRIMARY KEY (id)
);

