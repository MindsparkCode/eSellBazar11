create table feedback(
   id  int NOT NULL AUTO_INCREMENT,
   coustomerId int(25) NOT NULL,
   product_id int(20) NOT NULL,
   ditail varchar(270),
   rating int,
    PRIMARY KEY (id)
   );


drop table shipment;

create table shipment(
id  int NOT NULL AUTO_INCREMENT,
  order_id int(25) DEFAULT NULL,
  district varchar(25) NOT NULL,
  ditail varchar(255) NOT NULL,
  thana varchar(25) NOT NULL,
  postcode varchar(10) NOT NULL,
   PRIMARY KEY (id)
 

);

insert into shipment(district,thana,postcode,ditail) values("Dhaka","Mohammadpur","12345","11/9 shekdfjfhfj,ushdfki");

drop table vendor;
create table vendor(
  id  int NOT NULL AUTO_INCREMENT,
  name varchar(25),
  company  varchar(25),
  address varchar(25),
  district varchar(25),
  thana varchar(25) ,
  postcode varchar(10),
  nid varchar(25),
  phone varchar(25) ,
  email varchar(25) ,
  agriment varchar(25) ,
  date_time datetime default now(),
   PRIMARY KEY (id)
);

insert into vendor(name,company,address,district,thana,postcode,nid,phone,email,agriment) values("Kya Kosmetics","Kya Kosmetics","351 Mohakhali, Dhaka","Dhaka","Mohakhali","1212","05349785231","01712356489","kyatoertisltd.@gmail.com","Replesment grantty  within 7 days");
