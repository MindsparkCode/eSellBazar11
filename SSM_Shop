drop table emp;

create table emp(
  id  int NOT NULL AUTO_INCREMENT,
  name varchar(25),
  nid varchar(25),
  address varchar(25),
  emp_id varchar(255) NOT NULL,
  salary double(20,2) NOT NULL,
  bonus double(20,2) DEFAULT 0.00,
  commision double(20,2) DEFAULT 0.00,
  absentfee double(20,2) DEFAULT 0.00,
  post varchar(255) NOT NULL,
   PRIMARY KEY (id)
);
	



