select SUM(total) from orders where date_time between date_sub(now(),INTERVAL 1 WEEK) and now();



                               


insert into orderditais(order_id,coustomerId,product_id,product_name,image,color,size,category,
                     quantity,new_price,company,subtotal,status,cost_price,total_costprice)  values (" 
                    + order1.getId() + "," + id + "," + cart.getProduct_id() + ",'" + cart.getProduct_name()
					+ "','" + cart.getImage() + "','" + cart.getColor() + "','" + cart.getSize() + "','" +
					cart.getCategory() + "'," + cart.getQuntity() + "," + cart.getNew_price() + ",'" + cart.getCompany()
					+ "'," + cart.getNew_price() * cart.getQuntity() + ",'" + order1.getStatus() + "'," + 
					cart.getCost_price() +  "," + cart.getCost_price() * cart.getQuntity() +  ");



SELECT DISTINCT(district) FROM users;
SELECT count(district) FROM users where district="Dhaka";


select * from orders where id =4;







SELECT TO_CHAR(TIMESTAMP '2020-08-24 16:31:44', 'Mon');

SELECT TO_CHAR(TIMESTAMP date_time, 'Mon') as month from orders;
SELECT date_time, MONTHNAME(date_time)
FROM orders;
select sum(total) from orders where MONTHNAME(date_time)="January";
select DISTINCT(MONTHNAME(date_time)) from orders where YEAR(date_time)=2020 ORDER BY date_time;

select DISTINCT(MONTHNAME(date_time)) from orders where YEAR(date_time)=YEAR(CURDATE()) ORDER BY date_time;

select SUBSTRING(MONTHNAME(date_time),1,3), sum(total) from orders where MONTHNAME(date_time)="January" and YEAR(date_time)=2020;


SELECT DISTINCT(district) as district FROM users


SELECT * FROM projects WHERE YEAR(Date) = 2011 AND MONTH(Date) = 5

select MONTHNAME(date_time), sum(total) from orders where MONTHNAME(date_time)="January" and YEAR(date_time)=2020;

select SUBSTRING(DISTINCT(MONTHNAME(date_time)),1,3) from orders where YEAR(date_time)=YEAR(CURDATE()) ORDER BY date_time;

 

update products set image=select image from products+".jpg" where id=1;



UPDATE products 
SET image = CONCAT( image, '.jpg')
WHERE id=2; 
      



create table abc(
 id  int NOT NULL AUTO_INCREMENT,
  name varchar(25),
  nid varchar(25),
   PRIMARY KEY (id)
);

insert into abc(name,nid) value ("abc","def");


insert into abc(name,nid) value ("abc",CONCAT( nid, name));


insert into abc(name,nid) value ("xyz",CONCAT((SELECT AUTO_INCREMENT FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = DATABASE() AND TABLE_NAME = 'abc'), name));

 
 
 
 
 
 



 
  
  
	
	
