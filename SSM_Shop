==============================Project SSM Shop===============================
table name
------------------
product,
order,
wishlist,
users

create database ssmshop;
use ssmshop;






drop table wishlist;
drop table orders;
drop table users;
drop table products;
drop table colors;
drop table sizes;

create table users
         (id INT NOT NULL AUTO_INCREMENT,
		 fistname varchar(25),
		 lastname varchar(25),
		 roles  varchar(15),
		 email varchar(25),
		 phone varchar(15),
		 dirth date,
		 district varchar(15),
		 thana varchar(15),
		 address varchar(150),
		 password varchar(30),
		  PRIMARY KEY (id)
		 );
		 
