                color,company,category,size,new_price,cost_price, date(date_time) as date,
                time(date_time) time from cart where coustomerId=3;
   
   
create table orderditais(
   id  int NOT NULL AUTO_INCREMENT,
   order_id int DEFAULT NULL,
   coustomerId int DEFAULT NULL,
   product_id int DEFAULT NULL,
   product_name VARCHAR(25)  DEFAULT NULL,
   image VARCHAR(25) DEFAULT NULL,
   color VARCHAR(7) DEFAULT NULL,
   size varchar(5) DEFAULT NULL,
   category varchar(20) DEFAULT NULL,
   quantity DOUBLE(10,2) DEFAULT NULL,
   new_price DOUBLE(20,2) DEFAULT NULL,
   company VARCHAR(25) DEFAULT NULL,
   subtotal DOUBLE(20,2) DEFAULT NULL,
   status varchar(20) DEFAULT NULL,
   date_time datetime default now(),
    PRIMARY KEY (id)
   
);

ALTER TABLE orderditais
MODIFY COLUMN image VARCHAR(50);

ALTER TABLE orderditais
ADD cost_price DOUBLE(20,2) DEFAULT NULL;



ALTER TABLE orderditais
ADD total_costprice DOUBLE(20,2) DEFAULT NULL;


 SELECT SUM(total_costprice) from orderditais
WHERE MONTH(date_time) = MONTH(CURRENT_TIMESTAMP) AND YEAR(date_time) = YEAR(CURRENT_TIMESTAMP);


SELECT SUM(quantity) from orderditais
WHERE product_id= 6;


