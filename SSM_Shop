==============================Project SSM Shop===============================12/8/20( With out Foren key)
table name
------------------
product,
order,
wishlist,
users

create database ssmshop;
use ssmshop;




drop table feedback;
drop table emp;
drop table cart;
drop table wishlist;
drop table orders;
drop table users;
drop table products;

create table users
         (
		 id int NOT NULL AUTO_INCREMENT,
		 firstname varchar(25) DEFAULT NULL,
		 lastname varchar(25) DEFAULT NULL,
		 roles  varchar(15) DEFAULT "user",
		 email varchar(25)  NOT NULL,
		 phone varchar(15) DEFAULT NULL,
		 birth varchar(15) DEFAULT NULL,
		 district varchar(15) DEFAULT NULL,
		 thana varchar(15) DEFAULT NULL,
		 address varchar(150) DEFAULT NULL,
		 password varchar(30) NOT NULL,
		 UNIQUE KEY email (email),
		  PRIMARY KEY (id)
		 );
		 
drop table  products;
		
create table products(
   id int NOT NULL AUTO_INCREMENT,
   product_id varchar(20) DEFAULT NULL,
   product_name VARCHAR(70) DEFAULT NULL,
   image   VARCHAR(50)DEFAULT NULL,
   category varchar(50) DEFAULT NULL,
   old_price double(10,2) DEFAULT 0.00,
   new_price double(10,2) DEFAULT 0.00,
   cost_price double(10,2) DEFAULT 0.00,
   quantity DOUBLE(10,2) DEFAULT 0.00,
   cash_memo varchar(15) DEFAULT NULL,
   company varchar(50) DEFAULT NULL,
   vendormail varchar(70)  DEFAULT NULL,
   date_time datetime default now(),
   description varchar(500)DEFAULT NULL,
   stock_ditails varchar(200)DEFAULT NULL,
   UNIQUE KEY product_id (product_id),
    PRIMARY KEY (id)
);






drop table cart;

create table cart(
   id int NOT NULL AUTO_INCREMENT,
   coustomerId int DEFAULT NULL,
   product_id int DEFAULT NULL,
   p_id VARCHAR(25) DEFAULT NULL,
   product_name VARCHAR(25) DEFAULT NULL,
   image VARCHAR(50) DEFAULT NULL,
   color VARCHAR(25) DEFAULT NULL,
   vendormail VARCHAR(25) DEFAULT NULL,
   category varchar(7) DEFAULT NULL,
   size varchar(5)DEFAULT 'ALL',
   new_price DOUBLE(20,2) DEFAULT  0.00,
   cost_price double(10,2) DEFAULT 0.00,
   company VARCHAR(25) DEFAULT NULL,
   quntity DOUBLE(20,2) DEFAULT  0.00,
   date_time datetime default now(),
    PRIMARY KEY (id)
   
);



  
  
SELECT count(total_costprice) from orderditais WHERE MONTH(date_time) = MONTH(CURRENT_TIMESTAMP) AND YEAR(date_time) = YEAR(CURRENT_TIMESTAMP);

create table wishlist(
   id int NOT NULL AUTO_INCREMENT,
   customer_id int(25),
   product_id int(20),
   product_name VARCHAR(50)DEFAULT NULL,
   company varchar(50)DEFAULT NULL,
   new_price double(10,2) DEFAULT NULL,
    PRIMARY KEY (id)
);




drop table   orderditais;

drop table orders;
 
 

 
 
create table orders(
   id  int NOT NULL AUTO_INCREMENT,
   coustomerId int(25) NOT NULL,
   shipment DOUBLE(20,2) DEFAULT NULL,
   subtotal DOUBLE(20,2) DEFAULT NULL,
   sub_costtotal DOUBLE(20,2) DEFAULT NULL,
   vat DOUBLE(20,2) DEFAULT NULL,
   total DOUBLE(20,2) DEFAULT NULL,
   payment_method varchar(50) DEFAULT NULL,
   district varchar(25)  DEFAULT NULL,
   address varchar(255)  DEFAULT NULL,
   thana varchar(25)  DEFAULT NULL,
   
   postcode varchar(10)  DEFAULT NULL,
   contact varchar(20) DEFAULT NULL,
   status varchar(20) DEFAULT NULL,
   date_time datetime default now(),
    PRIMARY KEY (id)
  
);



ALTER TABLE orders
ADD sub_costtotal double(20,2) DEFAULT NULL;

 insert into orders (coustomerId,subtotal,sub_costtotal,vat,total,district,contact,date_time)
 values (2,7000,5000,100,7100,"Dhaka","017123232331"," 2020-02-15 01:53:58");

select SUM(total) from orders
where date_time between date_sub(now(),INTERVAL 1 WEEK) and now();



select SUM(total) from orders
where date_time between date_sub(now(),INTERVAL 2 WEEK) and (now(),INTERVAL 1 WEEK);

select  SUM(total) from orders  
WHERE date_time>= DATE_ADD(CURDATE(), INTERVAL -3 DAY);

 select  SUM(total) from orders where WEEK(CURDATE());
 
 
 SELECT SUM(total) from orders
WHERE MONTH(date_time) = MONTH(CURRENT_TIMESTAMP) AND YEAR(date_time) = YEAR(CURRENT_TIMESTAMP);









