(3,200,5000,50,5250,"Barishal","Mothbaria","Shekherteck","12586","Prosesing");

   
SELECT MAX(ID) as id,coustomerId,shipment,subtotal,vat,total,
 payment_method,district,address,thana,postcode,contact,
 status,date(date_time) as date,time(date_time) time FROM orders;
 
 
 
 SELECT MAX(ID) as id,coustomerId,shipment,subtotal,vat,total,
                 payment_method,district,address,thana,postcode,contact,
                 status,date(date_time) as date,time(date_time) time FROM orders where coustomerId=3;
   
        
